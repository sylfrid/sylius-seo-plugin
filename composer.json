{
    "name": "sylfrid/sylius-seo-plugin",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Sylius SEO plugin by Dedi.",
    "license": "MIT",
    "version": "1.2.0",
    "require": {
        "php": "^7.4",
        "sylius/sylius": "^1.7"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "fetzi/phpspec-watcher": "^2.0",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "lchrusciel/api-test-case": "^3.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpstan/phpstan-shim": "^0.11",
        "phpstan/phpstan-webmozart-assert": "^0.11",
        "phpunit/phpunit": "^6.5",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^2.0",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/intl": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dedi\\SyliusSEOPlugin\\": "src/",
            "Tests\\Dedi\\SyliusSEOPlugin\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
